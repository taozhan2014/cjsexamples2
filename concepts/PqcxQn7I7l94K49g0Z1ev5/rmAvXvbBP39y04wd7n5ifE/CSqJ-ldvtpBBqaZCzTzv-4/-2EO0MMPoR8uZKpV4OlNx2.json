  "activeScript" : "{\n    addIssue: function(puri, issue, templateUri) { \n        var newIssue = $cjs.copyConcept({\n            puri: puri,\n            curi: templateUri\n        });\n        newIssue.name(issue.issue);\n        newIssue.category(\".issue\");\n        newIssue.prop(\"issue\", issue.issue);\n        newIssue.prop(\"audience\", issue.audience);\n        newIssue.prop(\"reason\", issue.reason);\n        newIssue.prop(\"createTime\", issue.createTime);\n        newIssue.prop(\"location\", issue.location);\n        \n        return newIssue.uri();\n    }\n}\n\n",
